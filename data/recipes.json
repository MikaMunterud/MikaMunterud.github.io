  {
    "htmlIndex": "./html-recept/avoEggsBenedict.html",
    "recipeName": "Avokado Eggs Benedict",
    "date": "2022-10-25",
    "categories": [
      { "category": "ingredient", "name": "Vegetarisk", "value": "vegetarian" },
      { "category": "typeOfMeal", "name": "Brunch", "value": "brunch" },
